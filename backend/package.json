{
    "name": "quantum-matrix-backend",
    "version": "1.0.0",
    "description": "Backend API for Quantum Matrix DeFi Portfolio Manager",
    "main": "dist/server.js",
    "type": "module",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "dev:doppler": "doppler run -- npm run dev",
        "build": "tsc",
        "start": "node dist/server.js",
        "start:doppler": "doppler run -- npm run start",
        "test": "vitest",
        "test:unit": "vitest run tests/unit",
        "test:integration": "vitest run tests/integration",
        "db:migrate": "tsx src/db/migrate.ts",
        "db:seed": "tsx scripts/seed-data.ts",
        "db:setup": "tsx scripts/setup-db.ts",
        "lint": "eslint src --ext .ts",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "keywords": [
        "defi",
        "portfolio",
        "api",
        "blockchain"
    ],
    "author": "Rahil Bhavan",
    "license": "MIT",
    "dependencies": {
        "@google/genai": "^1.34.0",
        "axios": "^1.13.2",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "ethers": "^6.16.0",
        "express": "^4.21.2",
        "express-rate-limit": "^7.5.0",
        "helmet": "^8.0.0",
        "ioredis": "^5.4.2",
        "node-cron": "^4.2.1",
        "pg": "^8.13.1",
        "rss-parser": "^3.13.0",
        "sentiment": "^5.0.2",
        "winston": "^3.17.0",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/node": "^22.14.0",
        "@types/node-cron": "^3.0.11",
        "@types/pg": "^8.11.10",
        "@types/sentiment": "^5.0.4",
        "@typescript-eslint/eslint-plugin": "^8.19.1",
        "@typescript-eslint/parser": "^8.19.1",
        "eslint": "^9.18.0",
        "prettier": "^3.4.2",
        "tsx": "^4.19.2",
        "typescript": "^5.8.2",
        "vitest": "^2.1.8"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}
